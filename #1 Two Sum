class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        int i,j,length;
        length=nums.size();
        for(i=0;i<length;i++)
            {
                for(j=i+1;j<length;j++)
                    {
                        if(nums[i]+nums[j]==target)
                        {
                            return {i,j};
                        }
                    }
            }
        return {};
    }
};
