class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        unordered_map <int, int> pair;
        for(int i = 0;i!= nums.size();i++){
            int s = target - nums[i];
            if(pair.find(s)!= pair.end()){
                return {pair[s],i};
            }
            pair[nums[i]] = i;
        }
        return {};
    }
};
